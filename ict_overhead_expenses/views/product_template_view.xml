<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
         <record id="ict_overhead_expense_product_template_tree_view" model="ir.ui.view">
             <field name="name">ict_overhead_expense.product.template.tree.view</field>
             <field name="model">product.template</field>
             <field name="inherit_id" ref="product.product_template_form_view" />
             <field name="arch" type="xml">
                 <xpath expr="//group[@name='accounting']" position="inside">
                    <field name="costing_item_ok"></field>
                    <field name="initial_stock" readonly="1"></field>
                 </xpath>
             </field>
         </record>
    </data>
</odoo>